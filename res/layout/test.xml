<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_height="match_parent"
              android:layout_width="match_parent"
              android:orientation="vertical"
              android:id="@+id/test_parent"
        >
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
        <w1.app.easymoney.view.RollingSelectorWithStyle
                android:id="@+id/test_listselector"
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:layout_height="250dp"
                android:background="@color/RollingSelector_bg"
                android:cacheColorHint="@android:color/transparent"
                >
        </w1.app.easymoney.view.RollingSelectorWithStyle>
        <w1.app.easymoney.view.RollingSelectorWithStyle
                android:id="@+id/test_listselector2"
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:layout_height="250dp"
                android:background="@color/RollingSelector_bg"
                android:cacheColorHint="@android:color/transparent"
                >
        </w1.app.easymoney.view.RollingSelectorWithStyle>
        <w1.app.easymoney.view.SelectorContainer
                android:id="@+id/test_loopselector"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="250dp">
        </w1.app.easymoney.view.SelectorContainer>
        <w1.app.easymoney.view.SelectorContainer
                android:orientation="horizontal"
                android:id="@+id/test_container"
                android:layout_width="match_parent"
                android:layout_height="250dp"
                android:layout_weight="1"
                android:background="#FF0000">
        </w1.app.easymoney.view.SelectorContainer>

    </LinearLayout>
    <Button android:id="@+id/test_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Click me"/>
    <FrameLayout android:layout_width="match_parent" android:layout_height="wrap_content">
        <Button android:id="@+id/test_button1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Click me1"
                android:visibility="gone"/>
        <Button android:id="@+id/test_button2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Click me2"/>
    </FrameLayout>

</LinearLayout>
