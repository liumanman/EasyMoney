<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">
    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#FFFFFF"></LinearLayout>

    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="114px"
            android:background="@android:color/transparent"
            android:gravity="right"
            >
        <LinearLayout android:layout_width="236px" android:layout_height="match_parent"></LinearLayout>
        <LinearLayout android:layout_width="19px" android:layout_height="match_parent"></LinearLayout>
        <LinearLayout android:layout_width="236px" android:layout_height="match_parent"></LinearLayout>
        <LinearLayout android:layout_width="19px" android:layout_height="match_parent"></LinearLayout>
        <LinearLayout android:layout_width="236px" android:layout_height="match_parent"></LinearLayout>
        <LinearLayout android:layout_width="19px" android:layout_height="match_parent"></LinearLayout>
        <LinearLayout android:layout_width="151px" android:layout_height="match_parent"
                      android:background="@drawable/basepopupwindow_tab_bg"></LinearLayout>
    </LinearLayout>
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="10px"
            android:background="#494848">
    </LinearLayout>
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="3px"
            android:background="#626262">
    </LinearLayout>
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >
    </LinearLayout>
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="3px"
            android:background="#626262">
    </LinearLayout>
</LinearLayout>